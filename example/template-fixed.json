{
  "appID": "mysql",
  "cmd": null,
  "cpus": 0.4,
  "mem": 500,
  "disk": 0,
  "runAs": "xcm",
  "priority": 100,
  "instances": 1,
  "constraints": "unique hostname",
  "container": {
    "docker": {
      "image": "mysql",
      "network": "swan",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
            "key": "label",
	    "value": "APP_ID=mysql"
        }
      ],
      "portMappings": [ ]
    },
    "type": "DOCKER",
    "volumes": [
    ]
  },
  "env": {
    "MYSQL_ROOT_PASSWORD": "root"
  },
  "labels": {
    "USER_ID": "1"
  },
  "killPolicy": {
    "duration": 5
  },
  "healthChecks": [
    {
      "protocol": "CMD",
      "value": "ls",
      "delaySeconds": 2,
      "gracePeriodSeconds": 5,
      "intervalSeconds": 1,
      "portName": "web",
      "timeoutSeconds": 1,
      "consecutiveFailures": 500
    }
 ],
  "mode": "fixed",
  "ip": ["192.168.1.210"]
}
